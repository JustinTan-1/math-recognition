{
    "manifest_version": 3,
    "name": "MathRecognition",
    "description": "Draw and copy math symbols",
    "version": "1.0",
    "icons": {
        "16": "./images/16.png",
        "48": "./images/48.png",
        "128": "./images/128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts" : [{
        "matches": ["<all_urls>"],
        "js": ["script.js"]
    }]
}